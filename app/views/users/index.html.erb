
 <div class="container">
	 <div class="row">
	 	<div class="col-xs-3">
			<%= render 'users/info',user: current_user %>
			<%= render 'books/form', book: @book %>
		</div>
		<div class="col-xs-9">
<h1>Users</h1>

<table>
	<thead>
		<tr>
			<th>image</th>
			<th>name</th>
			<th></th>
		</tr>
	</thead>



 <tbody>
 	 <% @users.each do |user| %>
	  <tr>
		<td> <%= attachment_image_tag user,:profile_image, size:"50x50",format:'jpeg', fallback: "no_image.jpg",size:'50x50'  %></td>
		<td><%= user.name %></td>
		<td><%= link_to 'Show', user_path(user) %></td>
	  </tr>
	<% end %>
 </tbody>
</table>
</div>
</div>

